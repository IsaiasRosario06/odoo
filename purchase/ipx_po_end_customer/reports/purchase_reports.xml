<odoo>
  <data>
    <template id="report_purchaseorder_document_inherit_end_customer" inherit_id="purchase.report_purchaseorder_document">
      <xpath expr="//t[@t-if='o.dest_address_id']" position='after'>
        <t t-if="o.end_customer_id">
          <strong>End Customer:</strong><br/>
          <span t-field="o.end_customer_id.name"/><br/>
          <div t-field="o.end_customer_id" t-options="{&quot;widget&quot;: &quot;contact&quot;, &quot;fields&quot;: [&quot;mobile&quot;, &quot;phone&quot;, &quot;email&quot;], &quot;no_marker&quot;: False, &quot;phone_icons&quot;: True}" />
          <!-- <span t-field="o.end_customer_id.street"/><br/>
          <span t-field="o.end_customer_id.city"/><br/>           -->
        </t> 
        <t t-if="o.end_contact_id">
            <br/>
            <strong>Contact:</strong>
            <div t-field="o.end_contact_id.name"/>
            <div t-field="o.end_contact_id" t-options="{&quot;widget&quot;: &quot;contact&quot;, &quot;fields&quot;: [&quot;mobile&quot;, &quot;phone&quot;, &quot;email&quot;], &quot;no_marker&quot;: False, &quot;phone_icons&quot;: True}" />
        </t>
      </xpath>
    </template>
    
    <template id="report_purchasequotation_document_inherit_end_customer" inherit_id="purchase.report_purchasequotation_document">
      <xpath expr="//t[@t-if='o.dest_address_id']" position='after'>
